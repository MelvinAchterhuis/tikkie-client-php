{
  "name": "optiosteam/tikkie-client-php",
  "description": "Tikkie API client library for PHP developed by Optios.",
  "keywords": [
    "tikkie",
    "client",
    "api",
    "php"
  ],
  "support": {
    "issues": "https://github.com/optiosteam/tikkie-client-php/issues",
    "source": "https://github.com/optiosteam/tikkie-client-php"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Optios BV",
      "email": "dev@optios.net"
    }
  ],
  "require": {
    "php": ">=8.1",
    "composer/ca-bundle": "^1.5",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "nesbot/carbon": "^3.7",
    "league/url": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Optios\\Tikkie\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Optios\\Tikkie\\": "tests/"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.10",
    "phpmd/phpmd": "^2.15",
    "phpunit/phpunit": "^11.3",
    "spatie/phpunit-snapshot-assertions": "^5.1"
  }
}
